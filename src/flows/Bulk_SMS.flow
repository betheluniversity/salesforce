    <decisions>
        <name>Check_For_Empty_Results</name>
        <label>Check For Empty Results</label>
        <locationX>1750</locationX>
        <locationY>596</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Empty_Results</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Total_Contacts</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>No Contacts Found</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Total_Contacts</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>FLOW ERROR:</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>No_Contacts_Found_Message</targetReference>
            </connector>
            <label>Empty Results</label>
        </rules>
        <rules>
            <name>Results_Returned</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Total_Contacts</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>No Contacts Found</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Report_Confirmation_Screen</targetReference>
            </connector>
            <label>Results Returned</label>
        </rules>
    </decisions>
