    <formulas>
        <name>Number_Of_Contacts</name>
        <dataType>Number</dataType>
        <expression>LEN({!Total_Contacts}) - LEN(SUBSTITUTE({!Total_Contacts}, &quot;;&quot;, &quot;&quot;))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>Term_Cap</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 720</expression>
    </formulas>
