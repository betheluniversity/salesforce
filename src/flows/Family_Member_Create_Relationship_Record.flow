    <decisions>
        <name>Interaction_Was_Created_with_Contact</name>
        <label>Interaction Was Created with Contact</label>
        <locationX>820</locationX>
        <locationY>670</locationY>
        <defaultConnectorLabel>Contact Lookup Does Not Exist</defaultConnectorLabel>
        <rules>
            <name>Contact_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Interaction.Interaction_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Imported</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Interaction.Contact__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_RelatedContactID_from_Interaction</targetReference>
            </connector>
            <label>Contact Lookup Exists</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Address_Different_Than_Contact</name>
        <label>Is Address Different Than Contact</label>
        <locationX>601</locationX>
        <locationY>209</locationY>
        <defaultConnector>
            <targetReference>Get_Contact</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>AddressDifferentYes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FamilyMemberRecord.Is_Address_Different_Than_Contact__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Yes</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Interaction</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Email_Empty</name>
        <label>Is Email Empty</label>
        <locationX>427</locationX>
        <locationY>126</locationY>
        <defaultConnector>
            <targetReference>Is_Address_Different_Than_Contact</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>YesEmailEmpty</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FamilyMemberRecord.Email__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_email_to_blackhole_if_blank</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>Is the Related Contact field on the new Family Member record null?</description>
        <name>Related_Contact_is_Null</name>
        <label>Related Contact is Null</label>
        <locationX>261</locationX>
        <locationY>204</locationY>
        <defaultConnector>
            <targetReference>Set_Related_Contact_Id_Variable</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FamilyMemberRecord.Related_Contact__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_Email_Empty</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
