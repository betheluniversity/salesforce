    <assignments>
        <description>If the email is blank on family member record, put in a @blackhole.bethel.edu email. It will be removed by workflows, but the Interaction and Contact will still be created.</description>
        <name>Set_email_to_blackhole_if_blank</name>
        <label>Set email to @blackhole if blank</label>
        <locationX>609</locationX>
        <locationY>17</locationY>
        <assignmentItems>
            <assignToReference>FamilyMemberRecord.Email__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>family@blackhole.bethel.edu</stringValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Is_Address_Different_Than_Contact</targetReference>
        </connector>
    </assignments>
